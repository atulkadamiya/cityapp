<div id="container">
<% if @share.errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(@share.errors.count, "error") %> prohibited this image from being saved:</h2>
    <ul>
    <% @share.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>

<%= form_for @share, :html => { :multipart => true } do |u| %>
  <%= u.label 'Upload Images' %>
